{
  "multipart": [
	{ "when": { "facing":"north" },
      "apply": { "model": "relics:block/oddish_pot" }
    },
	{ "when": { "facing":"east" },
      "apply": { "model": "relics:block/oddish_pot", "y": 90 }
    },
	{ "when": { "facing":"south" },
      "apply": { "model": "relics:block/oddish_pot", "y": 180 }
    },
    { "when": { "facing":"west" },
      "apply": { "model": "relics:block/oddish_pot", "y": 270 }
    }
  ]
}